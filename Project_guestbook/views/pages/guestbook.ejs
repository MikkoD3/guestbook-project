<html>
<title>Guestbook</title>
<head>
  <link href="pages/css/mycss.css" rel="stylesheet" type="text/css" />
<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
<script
src="http://code.jquery.com/jquery-3.1.1.js"
integrity="sha256-16cdPddA6VdVInumRGo6IbivbERE8p7CQR3HzTBuELA="
crossorigin="anonymous"></script>
<link href="pages/css/mycss.css" rel="stylesheet" type="text/css" />
</head>
<body>
<div class="panel panel-info">
<div class="panel-heading">
<h1>Guestbook</h1>
</div>
</div>
<div class="btn-group" role="group">
<a role="button" class="btn btn-info btn-lg" href="/newmessage">Go write your message</a>
<a role="button" class="btn btn-warning btn-lg" href="/">Homepage</a>
</div>
<br>
<br>
<br>
<div id="tabledata">
  <table class="table table-bordered">
    <tr class="active">
      <th>Guest Number</th>
      <th>Username</th>
      <th>Country</th>
      <th>message</th>
      <th>Date</th>
    </tr>
    <tr class="active">
       <% for (var i=0; i < guestbook.length; i++) { %>
          <td class="active"><%= guestbook[i].id %></td>
          <td class="active"><%= guestbook[i].username %></td>
          <td class="active"><%= guestbook[i].country %></td>
          <td class="active"><%= guestbook[i].message %></td>
          <td class="active"><%= guestbook[i].date %></td>
        </tr>
       <% } %>

  </table>
</div>


</body>
</html>
